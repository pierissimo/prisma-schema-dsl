{
  "name": "@pmaltese/davinci-prisma",
  "version": "0.1.1",
  "description": "Prisma generator module for DaVinci",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "format": "prettier --write src",
    "check-format": "prettier --check src"
  },
  "keywords": [
    "prisma",
    "schema",
    "dsl",
    "javascript",
    "ast",
    "generator"
  ],
  "author": {
    "name": "Pietro Maltese",
    "email": "maltesepiero@gmail.com",
    "url": "https://pieromaltese.me"
  },
  "license": "MIT",
  "dependencies": {
    "@pmaltese/prisma-schema-generator": "workspace:^"
  },
  "peerDependencies": {
    "@davinci/core": "^2.9.0",
    "@davinci/reflector": "^2.2.0",
    "pino": "^8.0.0"
  },
  "devDependencies": {
    "@davinci/core": "^2.9.0",
    "@davinci/reflector": "^2.2.0",
    "@types/jest": "^26.0.24",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "ts-toolbelt": "^9.6.0",
    "typescript": "^4.1.3",
    "pino": "^8.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "dist"
    ],
    "testMatch": [
      "**/__tests__/**/*.spec.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ]
  }
}
