{
  "name": "@pmaltese/prisma-schema-generator",
  "version": "5.0.0",
  "description": "JavaScript SDK to generate Prisma Schema file",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "format": "prettier --write src",
    "check-format": "prettier --check src"
  },
  "keywords": [
    "prisma",
    "schema",
    "dsl",
    "javascript",
    "ast",
    "generator"
  ],
  "author": {
    "name": "Pietro Maltese",
    "email": "maltesepiero@gmail.com",
    "url": "https://pieromaltese.me"
  },
  "license": "MIT",
  "dependencies": {
    "@prisma/internals": "^4.12.0",
    "lodash.isempty": "^4.4.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/lodash.isempty": "^4.4.7",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.6",
    "ts-toolbelt": "^9.6.0",
    "typescript": "^4.9.5"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "dist"
    ],
    "testMatch": [
      "**/__tests__/**/*.spec.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ]
  }
}
